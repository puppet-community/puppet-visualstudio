{
  "name": "puppet-visualstudio",
  "version": "4.0.1-rc0",
  "author": "Vox Pupuli",
  "license": "Apache-2.0",
  "summary": "Module for managing Microsoft Visual Studio",
  "source": "https://github.com/voxpupuli/puppet-visualstudio",
  "project_page": "https://github.com/voxpupuli/puppet-visualstudio",
  "issues_url": "https://github.com/voxpupuli/puppet-visualstudio/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "windows",
      "operatingsystemrelease": [
        "2008",
        "2008 R2",
        "2012",
        "2012 R2"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 10.0.0"
    },
    {
      "name": "puppetlabs/powershell",
      "version_requirement": ">= 1.1.1 < 7.0.0"
    },
    {
      "name": "liamjbennett/win_facts",
      "version_requirement": ">= 0.0.2 < 2.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 7.0.0 < 9.0.0"
    }
  ]
}
